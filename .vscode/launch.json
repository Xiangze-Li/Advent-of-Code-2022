  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Daily Puzzle",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "${input:Day}",
      ],
    }
  ],
  "inputs": [
    {
      "id": "Day",
      "type": "promptString",
      "description": "Day of the puzzle to run",
    },
  ],
}
